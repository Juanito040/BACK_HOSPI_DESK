            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Juan Pérez\",\n  \"email\": \"juan.perez@hospital.com\",\n  \"password\": \"Password123!\",\n  \"phone\": \"+573001234567\",\n  \"role\": \"ADMIN\"\n}"
